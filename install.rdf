<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>skts@github.com/oyvindio</em:id>
    <em:version>0.1</em:version>
    <em:type>2</em:type>

    <!-- Front End MetaData -->
    <em:name>Single Key Tab Switch</em:name>
    <em:description>Provides single hotkeys for tab switching.</em:description>
    <em:creator>Øyvind Øvergaard</em:creator>
    <em:homepageURL>http://github.com/oyvindio</em:homepageURL>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.5</em:minVersion>
        <em:maxVersion>3.5.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:optionsURL>chrome://skts/content/skts_prefs.xul</em:optionsURL>

  </Description>      
</RDF>
